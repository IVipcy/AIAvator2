{
  "version": 2,
  "name": "ai-avator2-pro4",
  "git": {
    "deploymentEnabled": {
      "master": true
    }
  },
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "pythonVersion": "3.9",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "regions": ["hnd1"],
  "analytics": {
    "speed": {
      "enabled": true
    }
  }
} 