{
  "version": 2,
  "name": "ai-avator2-production",
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "pip install -r requirements.txt",
        "installCommand": "python -m pip install --upgrade pip",
        "pythonVersion": "3.9",
        "root": ".",
        "outputDirectory": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
} 